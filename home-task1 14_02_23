#include<iostream>
#include"time.h"

/*


разделение одного массива на два массива 




*/
using namespace std;
int main() {
	

	int max = 0, min = 10, usernum = 0;
	const int size = 10,size1=5;
	cin >> usernum;
	int arr[size],arr1[size1], arr2[size];

	for (int j = 0; j < 10; j++)
	{
		arr[j] = 1 + rand() % 9;
		cout << arr[j];
	
	}
	cout << "\n";
	for (int i = 0; i < 5; i++)
	{
		arr1[i] = arr[i];
		cout << arr1[i] ;

	}
	cout << "\n";
	for (int k = 5; k < 10; k++)
	{
		arr2[k] = arr[k];
		cout << arr2[k] ;

	}

}
