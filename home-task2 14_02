#include<iostream>
#include"time.h"

/*


сумма двух массивов




*/
using namespace std;
int main() {


	int max = 0, min = 10, usernum = 0;
	const int size = 10, size1 = 5;
	cin >> usernum;
	int arr[size], arr1[size], arr2[size];

	for (int j = 0; j < 10; j++)
	{
		arr[j] = 1 + rand() % 4;
		arr1[j] = 1 + rand() % 4;
		cout << arr[j] << "\n" << arr[j];

	}
	cout << "\n";
	for (int i = 0; i < 5; i++)
	{
		arr2[i] = arr[i]+arr1[i];
		cout << arr2[i];

	}
	cout << "\n";
	/*for (int k = 5; k < 10; k++)
